	"type" : "sdk",
	"homepage": "https://github.com/zoho/zohocrm-php-sdk-2.0",
	"license": "Apache-2.0",
	"authors" : [
		{
			"name" : "Zoho CRM API Team",
			"email" : "support@zohocrm.com",
			"homepage" : "https://www.zoho.com/crm/help/customer-support.html",
